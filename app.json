{
  "expo": {
    "name": "Crossfit Book",
    "slug": "CrossfitBook",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "primaryColor": "#212121",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#212121"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "splash": {
        "mdpi": "./assets/splash.png",   
        "hdpi": "./assets/splashhdpi.png",   
        "xhdpi": "./assets/splashxhdpi.png",  
        "xxhdpi": "./assets/splashxxhdpi.png", 
        "xxxhdpi": "./assets/splashxxxhdpi.png"
      }
    }
  }
}